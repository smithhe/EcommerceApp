{
  "metadata": [
    {
      "src": [
        {
          "src": "../../",
          "files": [
            "Api/Ecommerce.Api/Ecommerce.Api.csproj",
            "Api/Ecommerce.Api/bin/Release/net8.0/Ecommerce.Api.dll",
            "Api/Ecommerce.FastEndpoints/Ecommerce.FastEndpoints.csproj",
            "Api/Ecommerce.FastEndpoints/bin/Release/net8.0/Ecommerce.FastEndpoints.dll",
            
            "Core/Ecommerce.Application/Ecommerce.Application.csproj",
            "Core/Ecommerce.Application/bin/Release/net8.0/Ecommerce.Application.dll",
            "Core/Ecommerce.Domain/Ecommerce.Domain.csproj",
            "Core/Ecommerce.Domain/bin/Release/net8.0/Ecommerce.Domain.dll",
            "Core/Ecommerce.Messages/Ecommerce.Messages.csproj",
            "Core/Ecommerce.Messages/bin/Release/net8.0/Ecommerce.Messages.dll",
            "Core/Ecommerce.Shared/Ecommerce.Shared.csproj",
            "Core/Ecommerce.Shared/bin/Release/net8.0/Ecommerce.Shared.dll",
            
            "Infrastructure/Ecommerce.Identity/Ecommerce.Identity.csproj",
            "Infrastructure/Ecommerce.Identity/bin/Release/net8.0/Ecommerce.Identity.dll",
            "Infrastructure/Ecommerce.Mail/Ecommerce.Mail.csproj",
            "Infrastructure/Ecommerce.Mail/bin/Release/net8.0/Ecommerce.Mail.dll",
            "Infrastructure/Ecommerce.PayPal/Ecommerce.PayPal.csproj",
            "Infrastructure/Ecommerce.PayPal/bin/Release/net8.0/Ecommerce.PayPal.dll",
            "Infrastructure/Ecommerce.Persistence/Ecommerce.Persistence.csproj",
            "Infrastructure/Ecommerce.Persistence/bin/Release/net8.0/Ecommerce.Persistence.dll",
            "Infrastructure/Ecommerce.Persistence/Ecommerce.Persistence.csproj",
            "Infrastructure/Ecommerce.Persistence/bin/Release/net8.0/Ecommerce.Persistence.dll",
            "Infrastructure/Ecommerce.Worker/Ecommerce.Worker.csproj",
            "Infrastructure/Ecommerce.Worker/bin/Release/net8.0/Ecommerce.Worker.dll"
          ]
        }
      ],
      "dest": "api",
      "groupBy": "Assembly"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}",
          "api/*.yml"
        ],
        "exclude": [
          "_site/**",
          "ToCMaker/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "templates/material"
    ],
    "globalMetadata": {
      "_appName": " Ecommerce App",
      "_appFaviconPath": "./images/favicon.ico",
      "_enableSearch": true,
      "_disableContribution": true,
      "pdf": false
    }
  }
}